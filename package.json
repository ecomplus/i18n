  "homepage": "https://github.com/ecomplus/i18n#readme",
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "babel-loader": "^8.4.1",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "standard": "^17.1.2",
    "commit-and-tag-version": "^9.6.0",
